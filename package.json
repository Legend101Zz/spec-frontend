    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.11",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "axios": "^0.24.0",
    "gatsby": "^4.4.0",
